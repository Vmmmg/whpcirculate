/**app.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow-down:before {
  content: "\e6eb";
}

.icon-arrow-right:before {
  content: "\e6ec";
}

.icon-auto:before {
  content: "\e6ed";
}

.icon-clock:before {
  content: "\e6ee";
}

.icon-arrow-lift:before {
  content: "\e6ef";
}

.icon-QRcode:before {
  content: "\e704";
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.my-color {
  color: #476AF6
}

#head{
  margin-top: 10rpx;
  width: 100%;
  height: 120rpx;
  text-align: center;
  vertical-align: middle;
  line-height: 120rpx;
  font-size: 48rpx;
}

.lineLimitLength {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; 
}

.page__hd {
    padding: 40px;
}

.page__bd {
    padding-bottom: 40px;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}